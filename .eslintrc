{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "modules": true,
  },
  "env": {
    "es6": true,
  },
  "rules": {
    "quotes": [ 2, "single" ],
    "no-undef": 0,
    "no-new": 2,
    "new-cap": 0,
    "strict": 2,
    "no-extra-semi": 2,
    "no-underscore-dangle": 0,
    "no-console": 2,
    "no-trailing-spaces": 0,
    "func-names": 0,
    "no-unused-expressions": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "id-length": 0,
  }
}
